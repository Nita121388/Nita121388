<UserControl
    x:Class="WPFTest.NitaUserControl.NitaControlSamplesControl.NitaEmojiPickerSamples"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:nita="clr-namespace:WPFTest.NitaCustomControl"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel x:Name="NitaButton" Orientation="Horizontal">
        <nita:NitaControlSample
            x:Name="_nitaEmojiPicker"
            Title="NitaEmojiPicker控件"
            Description="Content:NitaEmojiPicker|SizeType:Small">
            <nita:NitaEmojiPicker
                x:Name="EmojiPicker"
                Margin="0,0,0,4"
                FontSize="24"
                Selection="👩‍💻" />
        </nita:NitaControlSample>
    </StackPanel>
</UserControl>
